{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url",
    "VITE_DEBUG": "false"
  },
  "envs": {
    "production": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
